                <owl:onProperty rdf:resource="http://www.imi.kit.edu/standard-pass-ont#hasModelComponent"/>
                <owl:someValuesFrom rdf:resource="http://www.imi.kit.edu/standard-pass-ont#InteractionDescribingComponent"/>
